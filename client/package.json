{
  "name": "gestao-horas-client",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "serve -s build",
    "test": "vite --host",
    "startServe": "serve  --ssl-cert ../certs/cert.pem --ssl-key  ../certs/key.pem -s build -p 3001",
    "serveHelp": "serve  --help",
    "viteHelp": "vite build --help",
    "viteOptimize": "vite optimize --help",
    "dev": "vite",
    "build": "vite build --minify esbuild",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "": "serve -s build -L",
    "start:trace": "node --trace-deprecation ./node_modules/serve/build/main.js  -s build"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@eslint/js": "^9.9.1",
    "@mui/material": "^6.0.0",
    "@mui/x-date-pickers": "^7.14.0",
    "@reduxjs/toolkit": "^2.2.7",
    "@testing-library/jest-dom": "^6.5.0",
    "axios": "^1.7.5",
    "date-fns": "^3.6.0",
    "moment": "^2.29.4",
    "node-openssl-cert": "^0.1.43",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.1",
    "react-toastify": "^10.0.5",
    "react-widgets": "^5.8.4",
    "styled-components": "^6.1.12",
    "web-vitals": "^4.2.3"
  },
  "devDependencies": {
    "@types/react": "^18.3.4",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react": "^7.35.0",
    "fs-extra": "^11.2.0",
    "globals": "^15.9.0",
    "prop-types": "15.8.1",
    "serve": "^14.2.3",
    "vite": "^5.4.2",
    "vite-plugin-mkcert": "^1.17.6"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "//": [
